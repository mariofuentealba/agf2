<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="true" xmlns:local="*" width="100%" height="100%">
	<fx:Script>
		<![CDATA[
			public function myRendererFunction(item:*):IFactory
			{
				return new ClassFactory(DetailItemRenderer);
				
			}
		]]>
	</fx:Script>
	<s:VGroup horizontalCenter="0" verticalCenter="0" horizontalAlign="center" width="100%">
		<s:Label fontSize="14" fontWeight="bold" text="{data['indice']}"/>
		<local:CheckList id="listEmpresas" labelField="rso" dataProvider="{data['asociados']}" 
						 width="100%" height="100%" itemRendererFunction="myRendererFunction"/>
		<!--s:List id="listEmpresas" labelField="rso" dataProvider="{data['asociados']}">
			
		</s:List>
		<s:DataGrid id="dgEmpresas" dataProvider="{data['asociados']}">
		</s:DataGrid-->
	</s:VGroup>
	
		
	
</s:ItemRenderer>
